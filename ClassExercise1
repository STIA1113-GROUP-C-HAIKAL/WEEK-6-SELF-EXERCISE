package ClassExercise;

public class ClassExercise {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x = 4;
		int y = 6;
		
		if (x < y)
		{
			System.out.println("It is true that X is less than Y");
		}
		
		else
		{
			System.out.println("It is false that X is less than Y");
		}
	}

}
