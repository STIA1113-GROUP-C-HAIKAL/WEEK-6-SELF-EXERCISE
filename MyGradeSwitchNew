package mySwitch;

import java.util.Scanner;

public class MyGradeSwitchNew {

	public static void main(String[] args) {
		int mark;
		
		Scanner sc = new Scanner(System.in);
		System.out.print("Please insert your mark : ");
		mark = sc.nextInt();
		
		//Kalo integer di bagi ama 10, angkanya bakal di buletin jadi angka pas misal 55 / 10 = 5, jadi casenya bisa jalan
		switch (mark/10)
		{
		case 10  :
		case 9  :
			System.out.println("Excellent, Grade A");
			break;
			
		case 8 :
			System.out.println("Very good, Grade B");
			break;
			
		case 7 :
			System.out.println("Good, Grade C");
			break;
			
		case 6 :
			System.out.println("Good, Grade D");
			break;
			
		case 5 :
			System.out.println("Good, Grade E");
			break;
			
		case 4 :
			System.out.println("Pass, Grade F");
			break;
			
		case 3 :
		case 2 :
		case 1 :
			System.out.println("Fail.");
			break;
			
			default : 
				System.out.println("INVALID GRADE");
			
		}
		
		sc.close();

	}

}
